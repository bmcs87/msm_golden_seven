<div class="page-header">
  <h1>Actor #<%= @actor.id %></h1>
</div>

<dl class="dl-horizontal">
  <dt>Dob</dt>
  <dd><%= @actor.dob %></dd>

  <dt>Name</dt>
  <dd><%= @actor.name %></dd>

  <dt>Bio</dt>
  <dd><%= @actor.bio %></dd>

  <dt>Image</dt>
  <dd><img src="<%= actor.image_url %>"></dd>
  <!-- <dt>Image</dt> -->
  <!-- <dd> <= actor.image_url %></dd> -->

  <p><a href="/actors/destroy" class="btn btn-primary">Delete actor</a></p>

  <p><a href="/actors/edit_form" class="btn btn-primary">Edit actor</a></p>

</dl>
